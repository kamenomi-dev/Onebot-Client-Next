{
  "name": "onebot-client-next",
  "version": "0.0.4",
  "description": "A QQ bot library supports onebot v11 protocol based on LLOneBot",
  "main": "./lib/bundle.cjs.js",
  "module": "./lib/bundle.esm.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "npx mocha",
    "pub": "npm publish --access public",
    "docs": "npx typedoc src/index.ts",
    "clean": "rm -rf lib",
    "build": "npm run clean && npx rollup -c --bundleConfigAsCjs"
  },
  "keywords": [
    "qq",
    "qqbot",
    "onebot",
    "onebot-v11"
  ],
  "author": "Kamenomi",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/ws": "^8.5.11",
    "mocha": "^10.6.0",
    "rollup": "^2.45.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.26.4"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript-logging": "^2.2.0",
    "typescript-logging-log4ts-style": "^2.2.0",
    "ws": "^8.18.0"
  },
  "files": [
    "lib/**/*.*",
    "LICENSE",
    "readme.md"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  }
}
